{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
        "vue"
    ],
    "overrides": [
        {
            "files": [
                "src/**/*.js",
                "src/**/*.vue"
            ]
        }
    ],
    "rules": {}
}